	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="box1" class="Box1">
		<property name="message1" value="Burak Köken"></property>
		<property name="message2" value="Şeyma Yılmaz"></property>
	</bean>
	
	<bean id="box2" class="Box2" parent="box1">
